import java.util.*;
public class Main{
    public static void main(String a[]){
        int i,j;
        int n=5;
        for(i=1;i<=n;i++){
            for(j=0;j<n;j++){
                System.out.print((j%2)+" ");
            }
            System.out.println();
        }
    }
}

output
---------
0 1 0 1 0 
0 1 0 1 0 
0 1 0 1 0 
0 1 0 1 0 
0 1 0 1 0
