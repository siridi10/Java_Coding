import  java.util.*;
public class Main{
    public static void main(String a[]){
        char i;
        char j;
        for(i='E';i>='A';i--){
            for(j='E';j>='A';j--){
                System.out.print(" "+i);
            }
            System.out.println();
        }
    }
}

output
----------------
E E E E E
D D D D D
C C C C C 
B B B B B 
A A A A A
