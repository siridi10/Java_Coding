import  java.util.*;
public class Main{
    public static void main(String a[]){
        char i;
        char j;
        for(i='E';i>='A';i--){
            for(j='E';j>='A';j--){
                System.out.print(" "+j);
            }
            System.out.println();
        }
    }
}

output
-------
E D C B A
E D C B A
E D C B A
E D C B A
E D C B A
